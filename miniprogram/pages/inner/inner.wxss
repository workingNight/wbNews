page {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    background-color: #f4f4f4;
}
.inner-header {
    width: 100%;
    padding: 20rpx 40rpx 0;
    box-sizing: border-box;
    display: flex;
    font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
    background-color: #ffffff;
}
.inner-header_left {
    display: flex;
    flex-direction: column; 
    width: 394rpx;
    color: #989898;
    line-height: 64rpx;
    padding-bottom: 30rpx;
}
.inner-header_left .inner-header-mian-title {
    font-size: 36rpx;
    font-weight: 600;
    color: #000000;
    line-height: 48rpx;
    margin-top: 8rpx;
    margin-bottom: 20rpx;
    overflow: hidden;
    text-overflow:ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    width:394rpx;
}
.inner-header_left .inner-header-fundMan{
    font-size: 26rpx;
}
.inner-header_left .inner-header-focusNum{
    font-size: 24rpx;
}
.inner-header_right {
    float: right;
    width: 277rpx;
    padding-left: 76rpx;   
}
.inner-header_right .inner-header-right-img {
    width: 200rpx;
    height: 200rpx;
    background-color: #eeeeee;
    border-radius: 15rpx;
}